<?xml version="1.0"?><!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<!--Check ??-->
<hibernate-mapping>
    <class name="Models.etudiant_1" table="etudiant_1" check="(gpa >=0 AND 4.3 >= gpa), credits_complete >= 0">
        <id name="gpa" type="java.lang.Float" column="gpa" >
            <generator class="native"/>
        </id>
        <property name="programme" type="enums.Programme"/>
        <property name="credits_complete" type="java.lang.Integer"/>
<!--        <property name="gpa" type="java.lang.Float" />-->

        <many-to-one name="matricule" column="matricule" class="Models.Person"/>
    </class>
</hibernate-mapping>