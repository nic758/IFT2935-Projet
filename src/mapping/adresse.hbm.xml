        PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<!--Check ??-->
<hibernate-mapping>
    <class name="Tables.adresse" table="adresses">
        <composite-id>
            <key-property name="code_postal" column="code_postal"/>
