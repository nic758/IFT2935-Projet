<?xml version="1.0"?><!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<!--Check ??-->
<hibernate-mapping>
    <class name="Models.entreprise" table="entreprise">
        <id name="id_entreprise" type="java.lang.String" column="id_entreprise" >
            <generator class="native"/>
        </id>
        <property name="nom_entreprise" type="java.lang.String"/>
        <property name="courriel_entreprise" type="java.lang.String"/>
        <property name="telephone_entreprise" type="java.lang.String"/>
        <property name="participante" type="java.lang.Boolean"/>
        <property name="domain" type="java.lang.String"/>
    </class>
</hibernate-mapping>